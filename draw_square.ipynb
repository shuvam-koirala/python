   "source": [
    "# Drawing a square using turtle library."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "metadata": {},
   "outputs": [],
   "source": [
    "import turtle as t\n",
    "t.bgcolor(\"yellow\")\n",
    "t.screensize(300,400)\n",
    "t.shape(\"turtle\")\n",
    "point=t.Turtle()\n",
    "point.speed(1)\n",
    "for i in range (4):\n",
    "    point.forward(100)\n",
    "    point.right(90)\n",
    "t.done()    \n"
   ]
